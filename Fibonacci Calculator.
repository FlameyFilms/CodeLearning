using System;
// I decided to challenge myself and make a calculator for it. 
namespace Fibo{
    class program {
        static void Main(string[] args) {
        //First number of calculation.
        int num1 = 1;
        //Second number of calculation.
        int num2 = 1;
        //Third number of calculation, the sum.
        int num3; 
        //By changing the value of what multiplier is it runs the loop that many times. 
         for(int multiplier = 0; multiplier < 10; multiplier++) {
            //Adds the numbers into the sum.
            num3 = num1 + num2;
            //Takes the value of the first number and makes it the second.
            num1 = num2;
            //Takes the value of the second number and makes it the value of the old sum.
            num2 = num3;
            //Prints the sum of the calculation (The next number of the Fibonacci Sequence.)
          Console.WriteLine(num3);
            //Repeats until loop is over.
         }
        }
     }
  }
